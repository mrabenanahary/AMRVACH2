.r Idl_amr/getpict

set_plot,'ps'
device,/encapsulated,bits=8,/color
device,xsize=16,ysize=16,xoffset=0,yoffset=0
device,filename='kht.t4.4.col.eps'

loadct,3

plotmode='contfilln_amr'
func='rho'
noautodomain=0
plottitle=''

Xyouts,0.1,0.1,'!17 '

headerline=0
bottomline=0
!p.title=''
!x.title='!17x'
!y.title='!17y'

.r Idl_amr/plotfunc
device,/close
