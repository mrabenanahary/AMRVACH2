F90=mpifort
FFLAGS = -c
F90FLAGS = -free -implicitnone -fopenmp -O2  -axMIC-AVX512 -DCONFIG_BFLOAT_8 -DH5_USE_16_API -fPIC
LINK= $(F90)  $(F90FLAGS) 

F77=mpifort
FFLAGS77 = -c
F77FLAGS = -cpp -fixed -implicitnone -fopenmp -O2  -axMIC-AVX512 -DCONFIG_BFLOAT_8 -DH5_USE_16_API -fPIC
LINK77= $(F77)  $(F77FLAGS)
