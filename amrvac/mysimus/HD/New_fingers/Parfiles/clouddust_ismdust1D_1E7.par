 &filelist
        base_filename='output/all_dust/clouddust_ismdust1D_1E7_'
        saveprim=.true.
        autoconvert=.true.
        convert_type='vtuBCCmpi'
        nwauxio=3
/
        restart_from_file='output/all_dust/clouddust_ismdust1D_1E7_0006.dat'
        snapshotnext =0007
/





/



&savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave_log=3.15e5 ! (0.001 year)
        dtsave_dat=3.15e5
 /
 dtsave_dat = 33356409.519815207 !in second ( 1 year)
        ditsave_dat=1
        ditsave_log = 1
        ditsave_dat =1

 &stoplist
        dtmin=1.D-10
        it_max=100000!102800.2139037433
/

  reset_it=.true.

          it_init=0
        time_max=10540





 &methodlist
        time_integrator        =  'twostep'
        typepred1              =  20*'tvdlf'
        flux_scheme            =  20*'tvdlf'
        limiter                =  20*'minmod'
        !typesourcesplit        = 'ssfss'
        small_pressure         = 1.0d-6
        small_density          = 1.0d-5
        small_values_method    = 'average'
        small_values_daverage  = 8
        check_small_values     = .true.
        small_values_force_floor=.true.
        loglimit(1)            = .true.
        loglimit(5)            = .true.

 /

         loglimit(6)            = .true.
         loglimit(7)            = .true.
        tvdlfeps=0.5d0

 &boundlist
        nghostcells=2
        typeboundary_min1 = 'symm','asymm','symm','asymm','symm',
                            4*'symm',4*'asymm',4*'symm',4*'asymm'
        typeboundary_max1 = 5*'cont',16*'cont'
  /
        typeboundary_min2 = 5*'cont',8*'cont'
        typeboundary_max2 = 5*'cont',8*'cont'
/
        typeboundary_min3 = 8*'cont'
        typeboundary_max3 = 8*'cont'
 /

 &meshlist
        refine_criterion=3
        refine_max_level=7
        refine_threshold=20*0.05d0
        derefine_ratio=20*0.9d0
        w_refine_weight(1)=0.3d0
        w_refine_weight(3)=0.1d0
        w_refine_weight(5)=0.2d0
        w_refine_weight(6)=0.2d0
        w_refine_weight(7)=0.2d0


        domain_nx1= 320 !128
        xprobmin1 = 0.0d0
        xprobmax1 = 3e17!4000 ! 400.d0


        iprob     = 1




 /

        itfixgrid =0

 &paramlist
        typecourant='maxsum'
        courantpar=0.8d0
        slowsteps = 1000
        small_getdt_average= .true.
 /

 &hd_list
        hd_gamma             = 1.666666667
        SI_unit              = .false.  ! use cgs
        He_abundance         = 0.0d0                     !
        hd_dust              = .true.   ! dust on
        hd_radiative_cooling = .true.   ! cooling on

        hd_small_pressure         = 1.0d-6
        hd_small_density          = 1.0d-5
  /


  &rc_list
        ncool            = 4000
        coolcurve        = 'SPEX_DM'
        coolmethod       = 'exact'
        cfrac            = 1.d0
        Tfix             = .true.
  /


&dust_list
    dust_n_species           = 4
    dust_min_rho             = 1.0d-6
    gas_mu                   = 1.0d0
    dust_method              = 'Kwok' !'sticking'!'none'!
    dust_small_to_zero       = .true.
    dust_temperature         = 1.0d0
    dust_temperature_type    = 'ism'

    dust_source_split        = .false.

    dust_it_diff             = 500
    dust_large_var_dragforce = 'none'

  /
  dust_dt_small            = 1.0d-6


 &usr_list
        usrconfig%physunit_on         = .true.                     !>
        usrconfig%ism_on              = .true.                     !> set ism
        usrconfig%cloud_on            = .true.                     !> set cloud
        usrconfig%cloud_number         = 1                          !> number of cloud
        usrconfig%coordinate_system    = 'spherical_1.75D'          !> geometry in use
        !'cylindrical_2.5D'         !> geometry in use
 /

 &usr_physical_unit_list
  phys_unit%unit                = 'cgs'
  phys_unit%length              = 1.496e+13                   ! cm (1/3 parsec)
  phys_unit%time                = 1d7                       ! s (0.01 year)
  phys_unit%number_density      = 1e8                      ! cm^-3
 /


 &usr_dust_ism_list
  dust_config%unit             = 'cgs'      ! dust used physical unit in this name list
  dust_config%associate_med    = .true.     ! dust is associate to a medium where is set
  dust_config%grain_density    = 3.3,3.3    ! dust grain density  (g/cm^-3) (ref: (Draine & Lee 1984).)
  dust_config%min_limit_rel    = 1d-6       ! dust minimum relative limite compare to gas density
  dust_config%min_limit_abs    = 1.0E-28    ! dust abs limite  (g/cm^3)
  dust_config%max_limit_rel    = 1d2        ! dust maximum relative limite compare to gas density
  dust_config%tracer_on        = .false.    ! dust with or without  tracer
  dust_config%n_species        = 2          ! dust number of species
  dust_config%idust_first      = 1          ! dust indice of the first dust grain used in code
  dust_config%idust_last       = 2          ! dust indice of the last dust grain used in code
  dust_config%sizes            = 0.0        ! dust sizes  (cm) if 0.0 , it will use distrub_func
  dust_config%distrub_func     = 'powerlaw' ! dust distribution function
  dust_config%min_radius       = 5.0d-7!5.0d-7 !5.0d-4!    ! dust smaller radius (cm) (ref : Decin et al 2006)
  dust_config%max_radius       = 2.5d-5!2.5d-5! 2.5d-5     !   ! dust smaller radius (cm) (ref : Decin et al 2006)
  dust_config%power_a          = -3.5       ! dust power law for distribtion function accroding to dust size
  dust_config%extend           = 0.0        ! dust region of space where is set. 0 means will be set in user file
 /


 &usr_ism_list
  ism_config%unit             ='cgs'
  ism_config%density          = 0          ! ism density if it set to 0 it will use number_density
  ism_config%number_density   = 1.0d4 !  1.0d7      ! ism number density (1/cm^3) (ref: Ary the Boss)
  ism_config%velocity         = 0,0,0      ! ism  speed components
  ism_config%temperature      = 1.0d3      ! ism temperature (K) (ref: Ary the Boss)
  ism_config%pressure         = 0.0d0      ! ism pressure if is set to 0 it will use temperature
  ism_config%tracer_on        = .false.    ! ism tracer
  ism_config%dust_on          = .true.    ! dust are set in ism
  ism_config%dust_frac        = 1d-2      ! dust fraction
/


&usr_cloud_list
 cloud_config%unit             = 'cgs'
 cloud_config%center(1)        = 15d14         ! cloud center position  (x in cartesian) (r in cylindrical) (cm)
 cloud_config%center(2)        = 0.0d0         ! cloud center position  (y in cartesian) (z in cylindrical) (cm)
 cloud_config%extend(1)        = 7.48d14       ! cloud size  (x in cartesian) (r in cylindrical) (cm) (ref: Ary the Boss)
 cloud_config%extend(2)        = 0.0d0         ! cloud size  (y in cartesian) (z in cylindrical) (cm)

 cloud_config%density          = 0.0d0         ! cloud density if it set to 0 it will use number_density or mass
 cloud_config%number_density   = 0.0d0         ! cloud number density (1/cm^3) if it set to 0 it will use mass
 cloud_config%mass             = 0.0596541d33  ! cloud mass (g)  (ref: Ary the Boss)
 cloud_config%shape            = 'sphere'      ! cloud shape
 cloud_config%profile          = 'uniform'     ! cloud profile
 cloud_config%velocity(1)      = 500d5            ! cloud speed in (x in cartesian) (r in cylindrical) (cm/s) (ref: Ary the Boss)
