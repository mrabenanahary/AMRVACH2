 cloud_config%velocity(3)      = 0             ! cloud speed in (z in cartesian) (phi in cylindrical) (cm/s)
 cloud_config%temperature      = 1            ! cloud temperature (K) (ref: Ary the Boss)
 cloud_config%pressure         = 0.0d0         ! cloud pressure if is set to 0 it will use temperature
 cloud_config%tracer_on        = .false.       ! cloud tracer
 cloud_config%dust_on          = .true.        ! dust are set in cloud
 cloud_config%dust_frac        = 1.0d-2!1.0d-2        ! dust fraction in cloud
 cloud_config%dust_profile     = 'uniform'     ! cloud profile
/

&usr_dust_cloud_list
