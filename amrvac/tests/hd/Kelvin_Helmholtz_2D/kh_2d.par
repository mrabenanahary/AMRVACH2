 &filelist
   base_filename    ='kh_2d'
   typefilelog = 'regression_test'
   autoconvert      = .false.
   convert_type     = 'vtuBCCmpi'
   saveprim         = .true.
 /

 &savelist
   dtsave_log       = 1.d-3
 /

 &stoplist
   time_max         = 5.d-2
 /

 &methodlist
   time_integrator     = 'threestep'
   flux_scheme       = 20*'hllc'
   limiter    = 20*'cada3'
 /

 &boundlist
   typeboundary_min1 = 4*'periodic'
   typeboundary_max1 = 4*'periodic'
   typeboundary_min2 = 4*'periodic'
   typeboundary_max2 = 4*'periodic'
 /

 &meshlist
   refine_max_level = 3
   domain_nx1       = 32
   domain_nx2       = 32
   block_nx1        = 16
   block_nx2        = 16
   xprobmin1        = 0.0d0
   xprobmax1        = 1.0d0
   xprobmin2        = 0.0d0
   xprobmax2        = 1.0d0
   iprob            = 1
 /

 &paramlist
   slowsteps        = 10
   courantpar       = 1.d0
   typecourant      = 'maxsum'
 /
