!setup.pl -d=2
 &filelist
        typefilelog='regression_test'
        base_filename='bw_2d'
        saveprim=.true.
        convert_type='vtuBCCmpi'
        autoconvert=.true.
        nwauxio=1
 /

 &savelist
        dtsave_log       = 1.d-3
 /

 &stoplist
        time_max            = 5.d-2
/

 &methodlist
        time_integrator=  'threestep'
        flux_scheme=    20*'hllc'
        limiter= 20*'koren'
 /

 &boundlist
        typeboundary_min1=4*'cont'
        typeboundary_max1=4*'cont'
        typeboundary_min2=4*'cont'
        typeboundary_max2=4*'cont'
 /

 &meshlist
        refine_criterion=3
        refine_max_level=3
        w_refine_weight(1)=0.5d0
        w_refine_weight(4)=0.5d0
        block_nx1=16
        block_nx2=16
        domain_nx1=32
        domain_nx2=32
        iprob=1 
        xprobmin1=0.1d0
        xprobmax1=2.1d0
        xprobmin2=0.d0
        xprobmax2=2.d0
        stretched_grid=.true.
        stretched_dim(1)=.true.
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.5d0
 /
