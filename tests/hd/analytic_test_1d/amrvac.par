 &filelist
        base_filename='analytic'
        typefilelog='special'
    	convert_type='vtuBCCmpi'
        autoconvert=T
/

 &savelist
        dtsave_log=4.0d-1
        ! dtsave_dat=0.1d-1
        ! tsave_dat=0.0d0
 /

 &stoplist
        time_max=0.4d0
 /

 &methodlist
        time_integrator = 'threestep'
        flux_scheme = 'hll'
        limiter='vanleer'
 /

 &boundlist
        typeboundary_min1 = 3*'symm'
        typeboundary_max1 = 3*'symm'
 /

 &meshlist
        refine_max_level=1
        refine_threshold=20*0.1d0
        block_nx1=16
        domain_nx1=256
        iprob=2
        xprobmin1=0.0d0
        xprobmax1=5.0d0
 /

 &paramlist
       courantpar=0.5
 /
