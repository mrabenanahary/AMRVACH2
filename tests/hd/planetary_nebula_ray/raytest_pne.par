 &filelist
        primnames='rho v1 v2 p'
        typeparIO=0
	saveprim = .true.
	autoconvert = .true.
	filenamelog='./output/amrvac'
	filenameout = './output/fixsmall'
        convert = .true.
	convert_type='vtuCC'
	!nocartesian=.false.
        !snapshotini=1
        !dxfiletype='lsb'
        !snapshotnext=36
        nwauxio=0
	filenameini= './output/fixsmall'
/
		


 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        ditsave(1)= 100
!        ditsave(2)= 1
        dtsave(2)= 5.0d-3
	
	
/
 &stoplist
!      itreset = .true.
!        itmax=100
       tmax = 5.0d-2
       dtmin= 1.0d-12
 /

 &methodlist
        wnames      = 'rho m1 m2 e'
        fileheadout = 'raytest'
        typeadvance = 'twostep'
        typefull1   = 13*'hllc'
        typepred1   = 13*'hllc'
        typetvdlf   = 'other'
        typelimiter1= 13*'koren'
        typelimited = 'previous'
	typeaxial   = 'spherical'
	useprimitive= .true.
	strictzero  = .true.
        strictsmall = .false.
        strictgetaux= .false.
        nflatgetaux = 2
        dimsplit    = .false.
	loglimit(1) = .true.
	loglimit(4) = .true.
	smallp      = 1.0d-12
	smallrho    = 1.0d-12
        fixprocess  = .true.
	fixsmall    = T
        ncool       = 5000
        coolmethod = 'exact'
        coolcurve  = 'MLsolar1'
 /

 &boundlist
        dixB=    2
        typeB=   4*'special',
                 4*'cont',
		 4*'periodic',
		 4*'periodic'	 
          
 /

 &amrlist
        iprob=2
        mxnest=4
	nxlone1=24
	nxlone2=48
	xprobmin1=0.025D0
	xprobmax1=0.25D0
	xprobmin2=0.125d0
	xprobmax2=0.375d0
	errorestimate=3
	flags(5)=1
	flags(1)=5
	wflags(1)=1.0D0
	tol = 0.3d0,0.2d0,11*0.1d0
	tolratio = 13*0.25D0
	prolongprimitive=.true.
	coarsenprimitive=.true.
 /

 &paramlist
        dtpar=-1
        typecourant='maxsum'
        courantpar=0.8d0
	cfrac = 0.1d0
        slowsteps=10
/

