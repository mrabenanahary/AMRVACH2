! setup.pl -d=22 -phi=0 -z=0 -g=14,14 -p=hd -eos=default -nf=0 -ndust=0 -u=nul -arch=default

 &filelist
        filenamelog='datars'
        filenameout='datars'
        primnames='rho v1 v2 p'
        saveprim=.true.
        autoconvert=.true.
        convert_type='vtuBCCmpi'
 /

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave=0.03d0,1.d0
 /

 &stoplist
        dtmin=1.D-6
!        tmax=100.d0
        itmax=1
 /

 &methodlist
        wnames=       'rho m1 m2 e'
        typeadvance=  'threestep'
        typefull1=    13*'tvdlf'
        typepred1=    13*'tvdlf'
        typelimiter1= 13*'woodward'
        typelimited='predictor'
 /

 &boundlist
        dixB=    2
        typeB=   4*'cont',4*'cont',4*'cont',4*'cont'
 /

 &amrlist
        errorestimate=3
        mxnest=1
        nbufferx1=0
        nbufferx2=0
        tol=13*0.2d0
        tolratio=13*0.125d0
        nxlone1=400
        nxlone2=400
        iprob=1
        xprobmin1=-100.d0
        xprobmax1=100.d0
        xprobmin2=-100.d0
        xprobmax2=100.d0
 /

 &paramlist
        typecourant='maxsum'
        slowsteps=10
        courantpar=0.8d0
 /
